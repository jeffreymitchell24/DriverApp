{
  "extends": ["standard", "standard-react"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "globals": {
    "__DEV__": true
  },
  "rules": {
    "react/jsx-sort-props": [2, { "callbacksLast": true }],
    "react/sort-prop-types": [2, { "callbacksLast": true }],
    "space-before-function-paren": "off",
    "jsx-quotes": "off",
    "space-in-parens": "off",
    "generator-star-spacing": "off"
  }
}
